.van-status{line-height:20px}.van-status .van-status-icon{display:inline-block;width:6px;height:6px;border-radius:50%;vertical-align:2px}.van-status .van-status-icon.default{background:#d9d9d9}.van-status .van-status-icon.success{background:#52c41a}.van-status .van-status-icon.error{background:#f5222d}.van-status .van-status-icon.processing{background:#1890ff;animation:processing 3s infinite ease-out}.van-status .van-status-icon.warning{background:#faad14}.van-status text{font-size:14px;color:rgba(0,0,0,0.75);margin-left:6px}@keyframes processing{0%{box-shadow:0 0 0 0 rgba(24,144,255,0.45)}80%{box-shadow:0 0 0 4px rgba(24,144,255,0)}100%{box-shadow:0 0 0 4px rgba(24,144,255,0)}}