<view class="van-result">
	<icon type="{{ status }}" class="van-result-icon" size="80" />
	<view class="van-result-title">{{ title }}</view>
	<view class="van-result-subtitle" wx:if="{{ subtitle }}">{{ subtitle }}</view>
	<view class="van-result-extra" wx:if="{{ useExtraSlot }}">
		<slot name="{{ extra }}" />
	</view>
</view>